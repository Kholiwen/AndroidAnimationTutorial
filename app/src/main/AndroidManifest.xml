        <activity android:name=".animactivity.LastActivity">
        </activity>
        <activity android:name=".animfragment.FragmentMainActivity">
        </activity>
        <activity android:name=".LayoutTransitionActivity">
        </activity>
        <activity
            android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>